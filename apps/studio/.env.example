# Generated by `pnpm env:sync:examples`.
# Source of truth: scripts/env/manifest.mjs.
# For real values, copy keys into .env.local.

# -----------------------------------------------------------------------------
# Core runtime
# Payload and app URL configuration for Studio runtime.
# -----------------------------------------------------------------------------
# Payload auth secret. Required in: local, preview, production.
PAYLOAD_SECRET=dev-secret-change-me

# Database connection URI. Optional in local (sqlite default), required in preview/production. Required in: preview, production.
# DATABASE_URI=file:./data/payload.db

# Public Studio URL used in redirects and checkout fallbacks. Required in: preview, production.
# NEXT_PUBLIC_APP_URL=http://localhost:3000

# -----------------------------------------------------------------------------
# Local developer bootstrap
# Local-only auth bootstrap and seed defaults.
# -----------------------------------------------------------------------------
# Enable local-only auto-login bootstrap for seeded admin (set 0 to disable). Required in: local.
NEXT_PUBLIC_LOCAL_DEV_AUTO_ADMIN=1

# Local auto-admin email used by LocalDevAuthGate. Required in: local.
NEXT_PUBLIC_LOCAL_DEV_AUTO_ADMIN_EMAIL=admin@forge.local

# Local auto-admin password used by LocalDevAuthGate. Required in: local.
NEXT_PUBLIC_LOCAL_DEV_AUTO_ADMIN_PASSWORD=admin12345

# Optional admin seed override for local data bootstrap.
SEED_ADMIN_EMAIL=admin@forge.local

# Optional admin seed password override.
SEED_ADMIN_PASSWORD=admin12345

# Optional non-admin user seed override.
SEED_USER_EMAIL=user@forge.local

# Optional non-admin user seed password override.
SEED_USER_PASSWORD=user12345

# -----------------------------------------------------------------------------
# AI and model routing
# OpenRouter and optional Copilot dev/public key settings.
# -----------------------------------------------------------------------------
# OpenRouter API key for chat, planning, structured output, and image routes. Required in: preview, production.
# OPENROUTER_API_KEY=

# Optional OpenRouter base URL override.
# OPENROUTER_BASE_URL=https://openrouter.ai/api/v1

# Optional OpenRouter timeout in milliseconds.
# OPENROUTER_TIMEOUT_MS=60000

# Enable/disable responses compatibility probing (1 enabled, 0 disabled).
# OPENROUTER_RESPONSES_PROBE=1

# Optional CopilotKit public key for dev tooling.
# NEXT_PUBLIC_COPILOTKIT_API_KEY=

# -----------------------------------------------------------------------------
# Stripe and billing
# Checkout, webhooks, and storage add-on billing variables.
# -----------------------------------------------------------------------------
# Stripe secret API key. Required in: preview, production.
# STRIPE_SECRET_KEY=

# Stripe webhook signing secret. Required in: preview, production.
# STRIPE_WEBHOOK_SECRET=

# Stripe price id for Pro subscription checkout. Required in: preview, production.
# STRIPE_PRICE_ID_PRO=

# Stripe price id for storage add-on checkout. Required in: preview, production.
# STRIPE_PRICE_ID_STORAGE_ADDON=

# Storage bytes granted per storage add-on checkout.
# STORAGE_ADDON_BYTES=53687091200

# Optional Stripe account id reference used in docs/ops.
# STRIPE_ACCOUNT_ID=

# Optional support contact for Stripe product metadata.
# STRIPE_SUPPORT_EMAIL=you@example.com

# Optional support URL for Stripe product metadata.
# STRIPE_SUPPORT_URL=https://example.com/support

# Optional privacy URL for Stripe/legal metadata.
# PRIVACY_POLICY_URL=https://example.com/privacy-policy

# Optional terms URL for Stripe/legal metadata.
# TERMS_OF_SERVICE_URL=https://example.com/terms-of-service

# -----------------------------------------------------------------------------
# Media upload and voice
# S3-compatible upload and ElevenLabs voice integration.
# -----------------------------------------------------------------------------
# S3-compatible bucket name for Better Upload.
# BETTER_UPLOAD_BUCKET=

# S3-compatible access key id.
# AWS_ACCESS_KEY_ID=

# S3-compatible secret access key.
# AWS_SECRET_ACCESS_KEY=

# S3-compatible region.
# AWS_REGION=us-east-1

# Optional legacy S3 bucket fallback key.
# AWS_S3_BUCKET=

# ElevenLabs API key for voice endpoints.
# ELEVENLABS_API_KEY=

# -----------------------------------------------------------------------------
# Analytics and logging
# PostHog analytics/flags and structured logging controls.
# -----------------------------------------------------------------------------
# Optional PostHog project key.
# NEXT_PUBLIC_POSTHOG_KEY=

# PostHog host URL.
# NEXT_PUBLIC_POSTHOG_HOST=https://us.i.posthog.com

# Logger level (debug, info, warn, error).
# LOG_LEVEL=info

# Optional log file path for Studio server logs.
# LOG_FILE=.logs/studio.log

# Optional comma-separated namespace filter for logging.
# LOG_NAMESPACES=

# Allow browser log relay endpoint in development (1 enabled).
ALLOW_CLIENT_LOG=0

# Enable browser log forwarding to /api/dev/log (1 enabled).
NEXT_PUBLIC_LOG_TO_SERVER=0

# -----------------------------------------------------------------------------
# Email and announcements
# Optional newsletter/notification sender variables.
# -----------------------------------------------------------------------------
# Optional Resend API key for newsletter welcome mail.
# RESEND_API_KEY=

# Sender email used by newsletter route when Resend is enabled.
# FROM_EMAIL=Forge <onboarding@resend.dev>

# -----------------------------------------------------------------------------
# Cross-origin platform auth
# Allowed origins for Platform -> Studio credentialed requests.
# -----------------------------------------------------------------------------
# Comma-separated allowlist for credentialed cross-origin API access.
# CORS_ALLOWED_ORIGINS=http://localhost:3001

# Optional single-origin alias for platform app URL.
# PLATFORM_APP_URL=http://localhost:3001
