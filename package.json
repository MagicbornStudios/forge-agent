{
  "name": "forge-agent",
  "private": true,
  "packageManager": "pnpm@9.15.5",
  "scripts": {
    "dev": "pnpm run build:showcase-registry && pnpm run env:bootstrap -- --app studio && pnpm --parallel --filter @forge/studio --filter @forge/docs dev",
    "dev:studio": "pnpm run env:bootstrap -- --app studio && pnpm --filter @forge/studio dev",
    "dev:docs": "pnpm run build:showcase-registry && pnpm --filter @forge/docs dev",
    "predev:repo-studio": "pnpm --filter @forge/repo-studio-app run codegen && pnpm forge-repo-studio doctor",
    "dev:repo-studio": "pnpm --filter @forge/repo-studio-app dev",
    "dev:consumer-studio": "pnpm --filter @forge/consumer-studio-app dev",
    "dev:platform": "pnpm run build:showcase-registry && pnpm run env:bootstrap -- --app platform && pnpm --filter @forge/platform dev",
    "dev:marketing": "pnpm run dev:platform",
    "build": "pnpm --filter @forge/studio build",
    "build:consumer-studio": "pnpm --filter @forge/consumer-studio-app build",
    "build:docs": "pnpm docs:platform:doctor && pnpm docs:showcase:doctor && pnpm --filter @forge/docs build",
    "build:platform": "pnpm docs:platform:doctor && pnpm docs:showcase:doctor && pnpm --filter @forge/platform build",
    "build:packages": "pnpm --filter @forge/ui run build && pnpm --filter @forge/shared run build && pnpm --filter @forge/agent-engine run build && pnpm --filter @forge/assistant-runtime run build && pnpm --filter @forge/dev-kit run build",
    "start": "pnpm --filter @forge/studio start",
    "start:docs": "pnpm --filter @forge/docs start",
    "start:platform": "pnpm --filter @forge/platform start",
    "css:doctor": "node scripts/css-doctor.mjs",
    "css:doctor:studio": "node scripts/css-doctor.mjs studio",
    "css:doctor:platform": "node scripts/css-doctor.mjs platform",
    "css:doctor:marketing": "node scripts/css-doctor.mjs platform",
    "hydration:doctor": "node scripts/hydration-nesting-doctor.mjs",
    "guard:assistant-canonical": "node scripts/guard-assistant-canonical.mjs",
    "guard:workspace-semantics": "node scripts/guard-workspace-semantics.mjs",
    "docs:runtime:doctor": "node scripts/docs-runtime-doctor.mjs",
    "forge-loop": "node scripts/forge-loop/cli.mjs",
    "forge-loop:new-project": "node scripts/forge-loop/cli.mjs new-project",
    "forge-loop:discuss-phase": "node scripts/forge-loop/cli.mjs discuss-phase",
    "forge-loop:plan-phase": "node scripts/forge-loop/cli.mjs plan-phase",
    "forge-loop:execute-phase": "node scripts/forge-loop/cli.mjs execute-phase",
    "forge-loop:verify-work": "node scripts/forge-loop/cli.mjs verify-work",
    "forge-loop:progress": "node scripts/forge-loop/cli.mjs progress",
    "forge-loop:doctor": "node scripts/forge-loop/cli.mjs doctor",
    "forge-loop:migrate-legacy": "node scripts/forge-loop/cli.mjs migrate-legacy",
    "forge-loop:sync-legacy": "node scripts/forge-loop/cli.mjs sync-legacy",
    "forge-loop:interactive": "node scripts/forge-loop/cli.mjs interactive",
    "forge-loop:loop:list": "node scripts/forge-loop/cli.mjs loop:list",
    "forge-loop:loop:new": "node scripts/forge-loop/cli.mjs loop:new",
    "forge-loop:loop:use": "node scripts/forge-loop/cli.mjs loop:use",
    "forge-loop:test": "pnpm --filter @forge/forge-loop test",
    "forge-loop:package:build": "pnpm --filter @forge/forge-loop build",
    "forge-loop:package:test": "pnpm --filter @forge/forge-loop test",
    "forge-loop:package:publish": "pnpm --filter @forge/forge-loop publish",
    "forge-env": "node packages/forge-env/src/cli.mjs",
    "forge-env:init": "node packages/forge-env/src/cli.mjs init",
    "forge-env:doctor": "node packages/forge-env/src/cli.mjs doctor",
    "forge-env:portal": "node packages/forge-env/src/cli.mjs portal",
    "forge-env:reconcile": "node packages/forge-env/src/cli.mjs reconcile",
    "forge-env:sync-examples": "node packages/forge-env/src/cli.mjs sync-examples --write",
    "forge-env:test": "pnpm --filter @forge/forge-env test",
    "forge-env:package:build": "pnpm --filter @forge/forge-env build",
    "forge-env:package:test": "pnpm --filter @forge/forge-env test",
    "forge-env:package:publish": "pnpm --filter @forge/forge-env publish",
    "forge-repo-studio": "node packages/repo-studio/src/cli.mjs",
    "forge-repo-studio:open": "node packages/repo-studio/src/cli.mjs open",
    "forge-repo-studio:doctor": "node packages/repo-studio/src/cli.mjs doctor",
    "forge-repo-studio:status": "node packages/repo-studio/src/cli.mjs status",
    "forge-repo-studio:stop": "node packages/repo-studio/src/cli.mjs stop",
    "forge-repo-studio:processes": "node packages/repo-studio/src/cli.mjs processes",
    "forge-repo-studio:reclaim": "node packages/repo-studio/src/cli.mjs reclaim",
    "forge-repo-studio:reclaim:all": "node packages/repo-studio/src/cli.mjs reclaim --scope repo --force",
    "forge-repo-studio:codex-status": "node packages/repo-studio/src/cli.mjs codex-status",
    "forge-repo-studio:codex-login": "node packages/repo-studio/src/cli.mjs codex-login",
    "forge-repo-studio:codex-start": "node packages/repo-studio/src/cli.mjs codex-start",
    "forge-repo-studio:codex-stop": "node packages/repo-studio/src/cli.mjs codex-stop",
    "forge-repo-studio:codex-exec": "node packages/repo-studio/src/cli.mjs codex-exec",
    "forge-repo-studio:run": "node packages/repo-studio/src/cli.mjs run",
    "dev:repo-studio:clean": "pnpm forge-repo-studio:reclaim && pnpm dev:repo-studio",
    "forge-repo-studio:desktop:dev": "pnpm --filter @forge/repo-studio run desktop:dev",
    "forge-repo-studio:desktop:build": "pnpm --filter @forge/repo-studio run desktop:build",
    "forge-repo-studio:desktop:package:win": "pnpm --filter @forge/repo-studio run desktop:package:win",
    "forge-repo-studio:test": "pnpm --filter @forge/repo-studio test",
    "forge-repo-studio:package:build": "pnpm --filter @forge/repo-studio build",
    "forge-repo-studio:package:test": "pnpm --filter @forge/repo-studio test",
    "forge-repo-studio:package:publish": "pnpm --filter @forge/repo-studio publish",
    "docs:platform:generate": "node scripts/generate-platform-component-docs.mjs && node scripts/generate-shared-showcase-docs.mjs",
    "docs:platform:doctor": "node scripts/platform-docs-doctor.mjs",
    "build:showcase-registry": "node scripts/build-showcase-registry.mjs",
    "docs:showcase:generate": "node scripts/build-showcase-registry.mjs && node scripts/generate-showcase-code.mjs && node scripts/generate-shared-showcase-docs.mjs",
    "docs:showcase:doctor": "node scripts/showcase-doctor.mjs",
    "docs:doctor": "pnpm css:doctor && pnpm docs:runtime:doctor && pnpm docs:platform:doctor && pnpm docs:showcase:doctor && pnpm --filter @forge/docs lint",
    "lint": "pnpm guard:assistant-canonical && pnpm guard:workspace-semantics && pnpm css:doctor && pnpm hydration:doctor && pnpm docs:runtime:doctor && pnpm docs:platform:doctor && pnpm docs:showcase:doctor && pnpm --filter @forge/docs lint && pnpm --filter @forge/platform lint && pnpm --filter @forge/studio lint",
    "test": "pnpm --filter @forge/studio test",
    "verify-openrouter": "node scripts/verify-openrouter-model-routing.mjs",
    "env:sync:examples": "node scripts/env-sync-examples.mjs",
    "env:sync:examples:check": "node scripts/env-sync-examples.mjs --check",
    "env:setup": "node scripts/env-setup.mjs",
    "env:doctor": "node scripts/env-doctor.mjs",
    "env:ensure:local": "node scripts/env-ensure-local.mjs",
    "env:portal": "node scripts/env/env-portal.mjs",
    "env:bootstrap": "node scripts/env-bootstrap.mjs",
    "env:sync:vercel": "node scripts/env-vercel-sync.mjs",
    "payload:types": "pnpm --filter @forge/studio payload:types",
    "knip": "knip",
    "registry:start": "verdaccio --config ./verdaccio.yaml",
    "registry:forge:build": "pnpm --filter @forge/ui build && pnpm --filter @forge/shared build && pnpm --filter @forge/agent-engine build && pnpm --filter @forge/assistant-runtime build && pnpm --filter @forge/dev-kit build && pnpm --filter @forge/forge-loop build",
    "registry:forge:bump": "node scripts/bump-forge-packages.mjs",
    "registry:forge:publish:local": "node scripts/bump-forge-packages.mjs && npm publish --registry http://localhost:4873 ./packages/ui && npm publish --registry http://localhost:4873 ./packages/shared && npm publish --registry http://localhost:4873 ./packages/agent-engine && npm publish --registry http://localhost:4873 ./packages/assistant-runtime && npm publish --registry http://localhost:4873 ./packages/dev-kit && npm publish --registry http://localhost:4873 ./packages/forge-loop && npm publish --registry http://localhost:4873 ./packages/forge-env && npm publish --registry http://localhost:4873 ./packages/repo-studio",
    "registry:forge:loop:publish": "node scripts/bump-forge-packages.mjs packages/forge-loop packages/forge-env packages/repo-studio && pnpm --filter @forge/forge-loop build && npm publish --registry http://localhost:4873 ./packages/forge-loop && npm publish --registry http://localhost:4873 ./packages/forge-env && npm publish --registry http://localhost:4873 ./packages/repo-studio",
    "gsd:install": "node scripts/gsd-install.mjs",
    "vendor:gsd:build": "node -e \"require('fs').accessSync('vendor/get-shit-done/bin/install.js')\"",
    "vendor:gsd:publish:local": "node scripts/vendor-gsd-publish.mjs"
  },
  "devDependencies": {
    "knip": "^5.83.1",
    "tsup": "^8.5.0",
    "verdaccio": "^5.30.0"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.2.11",
      "@types/react-dom": "19.2.3"
    }
  },
  "dependencies": {
    "posthog-js": "^1.334.1"
  }
}
